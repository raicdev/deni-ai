# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Optional: Supabase Service Role Key (for admin operations)
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# External APIs
BRAVE_SEARCH_API_KEY=
UPLOADTHING_TOKEN=

# AI Provider API Keys
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key
GOOGLE_API_KEY=your_google_ai_api_key
OPENROUTER_API_KEY=your_openrouter_api_key
GROQ_API_KEY=your_groq_api_key
XAI_API_KEY=your_xai_api_key

# Redis Configuration (Optional)
# Required for resumable streams feature
# If not set, resumable streams will be disabled and fallback to regular streams
REDIS_URL=redis://localhost:6379
# Example for Redis Cloud: REDIS_URL=rediss://username:password@host:port

# Legacy Firebase (can be removed after migration)
# NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
# NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=
# NEXT_PUBLIC_FIREBASE_PROJECT_ID=
# NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=
# NEXT_PUBLIC_FIREBASE_API_KEY=
# FIREBASE_SERVICE_ACCOUNT_KEY=